datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

// TODO: Author column here

model Workspace {
  id          Int      @id @default(autoincrement())
  name        String
  description String
  icon        String   @default("")
  inTrash     Boolean  @default(false)
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
  emojiLogo   String
  banner      String
}
